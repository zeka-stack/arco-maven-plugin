<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <!-- Spring Boot 应用打包配置 -->
    <id>lib</id>
    <formats>
        <format>dir</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <!-- 排除依赖 -->
    <dependencySets>
        <dependencySet>
            <useProjectArtifact>false</useProjectArtifact>
            <outputDirectory>lib</outputDirectory>
            <scope>runtime</scope>
            #{excludes}
        </dependencySet>
    </dependencySets>
</assembly>
